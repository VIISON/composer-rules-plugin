{
    "name": "viison/composer-rules-plugin",
    "type": "composer-plugin",
    "license": "MIT",
    "description": "This free and open source MIT-licensed Composer plugin allows you to add additional processing rules to your Composer-based dependency installation.",
    "support": {
        "issues": "https://github.com/VIISON/composer-rules-plugin/issues"
    },
    "homepage": "https://github.com/VIISON/composer-rules-plugin",
    "require": {
        "composer-plugin-api": "1.0.0",
        "composer/installers": "1.0.18",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "composer/composer": "1.0.0-alpha9@dev"
    },
    "extra": {
        "class": ["Viison\\ComposerRulesPlugin\\Plugin"]
    },
    "autoload": {
        "psr-0": {
            "Viison\\ComposerRulesPlugin\\": "src/"
        }
    }
}
